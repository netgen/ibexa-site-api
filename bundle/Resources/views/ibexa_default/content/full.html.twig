{% extends no_layout == true ? view_base_layout : '@IbexaCore/pagelayout.html.twig' %}
{% block content %}
    <h2>{{ ibexa_content_name(content) }}</h2>
    {% for field in content.fieldsByLanguage(language|default(null)) %}
        <h3>{{ field.fieldDefIdentifier }}</h3>
        {{ ibexa_render_field(content, field.fieldDefIdentifier) }}
    {% endfor %}
{% endblock %}
